{
  "name": "jobber",
  "version": "1.0.0",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "babel src --presets=@babel/preset-env --extensions .ts --ignore src/**/*.spec.ts --out-dir build/cjs --source-maps",
    "build:esm": "babel src --presets=@babel/preset-env --extensions .ts --ignore src/**/*.spec.ts --out-dir build/esm --source-maps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NHKhanq/jobber-app-microservice.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    },
    "./icons": {
      "import": "./esm/icons/index.js",
      "require": "./cjs/icons/index.js"
    }
  },
  "types": "./src/index.d.ts",
  "dependencies": {
    "express": "^4.19.2",
    "typescript": "^5.5.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-typescript": "^7.24.7",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6"
  }
}
